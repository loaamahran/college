</ul>
</nav> 
<div class="container mt-4">
      <br />
      <table class="table">
        <thead>
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Prerequisets</th>  
            <th>Type<th>
            <th>Credits</th>  
            <th>Hall</th> 
            <th>Time</th>     
          </tr>
        </thead>
        <tbody>  
        <tr>
          
          <th class="courses"></th>
          {{#each courses}}
            <td>{{courses.id}}</td>
            <td>{{ courses.name}}</td>
            <td>{{ courses.prerequisites}}</td>
            <td>{{ courses.type}}</td>
            <td>{{ courses.credits}}</td>
            <td>{{ courses.hall}}</td>
            <td>{{ courses.time}}</td>
          {{/each}}
          </tr>
          
        </tbody>
      </table>
    </div>